<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="passkey.passviewController">
   <children>
      <ImageView fitHeight="114.0" fitWidth="120.0" layoutX="31.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true">
         <image>
